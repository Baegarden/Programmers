# 완전탐색 - Level 2 : 소수 찾기

from itertools import permutations

def isTrue(N):
    n = 2
    while n < N:
        if N % n == 0:
            return False
        n += 1
    return True
    
def solution(numbers):
    answer = 0
    arr = []
    for i in range(len(numbers)):
        arr.append(numbers[i])
        
    arr2 = []     
    for i in range(1, len(numbers)+1):
        for j in permutations(arr, i):
            arr2.append(int("".join(j)))
            
    arr3 = list(set(arr2))   
    for i in arr3:
        if i > 1 and isTrue(i):
            answer += 1        
    return answer
